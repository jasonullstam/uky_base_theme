{% include 'molecules-skip-nav' %}
{% include 'components-universal-header' %}
{% include 'components-global-header' %}
<main class="main">
	{{ page.breadcrumb }}

	{% block content_prefix %}
		{{ page.content_prefix }}
	{% endblock %}

	{% block main_content %}
		{% if page.sidebar|trim %}
			{% include 'molecules-slab-1x2' with {
				main: page.content,
				aside: page.sidebar
			} %}
		{% else %}
			{{ page.content }}
		{% endif %}
	{% endblock %}

	{% block content_suffix %}
		{{ page.content_suffix }}
	{% endblock %}
</main>
{% include 'components-global-footer' %}
{% include 'components-universal-footer' %}
